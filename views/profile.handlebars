<div class="container">
	<div class="row profile">







		<div class="col-lg-3 col-sm-12">
			<div class="box">
			</div>
		</div>


		<div class="col-lg-5 col-sm-12">
			<div class="box panel panel-default">
				<div class="box-header">
					<h4>
						<a href="/workout">{{user.first_name}}'s Workouts</a>
					</h4>
				</div>
				<div class="box-content panel-body">
					<ul class="weekdays">

						<li>
					<div class="panel-group mondayTable">
						<div class="panel panel-default">
							<div class="panel-heading">
								<h4 class="panel-title">
									Monday
									<!--<a data-toggle="collapse" href="#collapse1">Monday</a>-->
								</h4>
							</div>
							<div id="collapse1" class="panel-collapse collapse panel-body">
								<ul>
									{{# each workouts}}
									<li>{{this.monday}}</li>
									{{/each}}
								</ul>
							</div>
						</div>
					</div>
				</li>

				<li>
					<div class="panel-group">
						<div class="panel panel-default">
							<div class="panel-heading">
								<h4 class="panel-title">
									<a data-toggle="collapse" href="#collapse2">Tuesday</a>
								</h4>
							</div>
							<div id="collapse2" class="panel-collapse collapse panel-body">
								<ul>
									{{# each workouts}}
									<li>{{this.tuesday}}</li>
									{{/each}}
								</ul>
							</div>
						</div>
					</div>
				</li>

				<li>
					<div class="panel-group">
						<div class="panel panel-default">
							<div class="panel-heading">
								<h4 class="panel-title">
									<a data-toggle="collapse" href="#collapse3">Wednesday</a>
								</h4>
							</div>
							<div id="collapse3" class="panel-collapse collapse panel-body">
								<ul>
									{{# each workouts}}
									<li>{{this.wednesday}}</li>
									{{/each}}
								</ul>
							</div>
						</div>
					</div>
				</li>

				<Li>
					<div class="panel-group">
						<div class="panel panel-default">
							<div class="panel-heading">
								<h4 class="panel-title">
									<a data-toggle="collapse" href="#collapse4">Thursday</a>
								</h4>
							</div>
							<div id="collapse4" class="panel-collapse collapse panel-body">
								<ul>
									{{# each workouts}}
									<li>{{this.thursday}}</li>
									{{/each}}
								</ul>
							</div>
						</div>
					</div>
				</li>

				<li>
					<div class="panel-group">
						<div class="panel panel-default">
							<div class="panel-heading">
								<h4 class="panel-title">
									<a data-toggle="collapse" href="#collapse5">Friday</a>
								</h4>
							</div>
							<div id="collapse5" class="panel-collapse collapse panel-body">
								<ul>
									{{# each workouts}}
									<li>{{this.friday}}</li>
									{{/each}}
								</ul>
							</div>
						</div>
					</div>
				</li>

				<li>
					<div class="panel-group">
						<div class="panel panel-default">
							<div class="panel-heading">
								<h4 class="panel-title">
									<a data-toggle="collapse" href="#collapse6">Saturday</a>
								</h4>
							</div>
							<div id="collapse6" class="panel-collapse collapse panel-body">
								<ul>
									{{# each workouts}}
									<li>{{this.saturday}}</li>
									{{/each}}
								</ul>
							</div>
						</div>
					</div>
				</li>

				<li>
					<div class="panel-group">
						<div class="panel panel-default">
							<div class="panel-heading">
								<h4 class="panel-title">
									<a data-toggle="collapse" href="#collapse7">Sunday</a>
								</h4>
							</div>
							<div id="collapse7" class="panel-collapse collapse panel-body">
								<ul>
									{{# each workouts}}
									<li>
										{{this.sunday}}
									</li>
									{{/each}}
								</ul>
							</div>
						</div>
					</div>
				</li>

					<div class="box-header">
						<h4>Add workouts</h4>
					</div>
					<div class="box-contents panel-body">
						<form class="" action="/workout" method="post">
							<div class="form-group">
								<div class="form-group">
									<label>Monday</label>
									<input type="text" class="form-control" name="monday" value=" ">
								</div>

								<div class="form-group">
									<label>Tuesday</label>
									<input type="text" class="form-control" name="tuesday"  value=" ">
								</div>

								<div class="form-group">
									<label>Wednesday</label>
									<input type="text" class="form-control" name="wednesday" value=" ">
								</div>

								<div class="form-group">
									<label>Thursday</label>
									<input type="text" class="form-control" name="thursday"  value=" ">
								</div>

								<div class="form-group">
									<label>Friday</label>
									<input type="text" class="form-control" name="friday"  value=" ">
								</div>

								<div class="form-group">
									<label>Saturday</label>
									<input type="text" class="form-control" name="saturday"  value=" ">
								</div>

								<div class="form-group">
									<label>Sunday</label>
									<input type="text" class="form-control" name="Sunday"  value=" ">
								</div>



								<button type="submit" class="btn btn-default">Submit</button>
							</form>

							<hr>

							</div>
						</form>


					</div>



				</ul>
				</div>
				<div class="col-lg-3 col-md-3 hidden-sm hidden-xs">
					<div class="panel panel-default">
						<div class="panel-body">
							<div class="media">
								<div class="media-body">
									<h4>
										<strong>{{user.first_name}} {{user.last_name}}</strong>
										<a href="/editProfile" class="btn btn-default btn-sm">profile setting</a>
									</h4>
									{{user.user_email}}
									<hr>
									<h4>
										<strong>Age</strong>
									</h4>
									{{user.user_age}}
									<hr>
									<h4>
										<strong>Gender</strong>
									</h4>
									{{user.user_gender}}
									<hr>
									<h4>
										<strong>Weight</strong>
									</h4>
									{{user.user_weight}} lb
									<hr>
									<h4>
										<strong>Fitness Goal</strong>
									</h4>
									{{user.fitness_goals}}
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>

	</div>

</div>
